{
    "name": "leovie/try-psalm",
    "description": "try psalm",
    "type": "project",
    "authors": [
        {
            "name": "Leo Viezens",
            "email": "leo.viezens@web.de"
        }
    ],
    "autoload": {
        "psr-4": {
            "LeoVie\\TryPsalm\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LeoVie\\TryPsalm\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "vimeo/psalm": "^4.3"
    },
    "config": {
        "optimize-autoloader": true,
        "bin-dir": "bin"
    },
  "require": {
    "ext-pdo": "*"
  }
}
